    "periods=14\n",
    "def williamsR(high, low, close, time):\n",
    "  highesth=high.rolling(time).max()\n",
    "  lowestl=low.rolling(time).min()\n",
    "\n",
    "  williams=((highesth-close)/(highesth-lowestl))*(-100)\n",
    "\n",
    "  return williams\n",
    "\n",
    "df['wr'] = williamsR(df['high'], df['low'], df['close'], periods)\n",
    "\n",
    "df"
   ]
